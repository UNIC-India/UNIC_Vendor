                    android:textSize="15sp"
                    android:paddingBottom="8dp"
                    app:layout_constraintLeft_toLeftOf="@id/gd6"
                    app:layout_constraintRight_toRightOf="@id/gd7"
                    app:layout_constraintTop_toBottomOf="@+id/title_mission" />

