                    android:lineSpacingExtra="4dp"
                    android:text="We strive to create a seamless selling and buying experience for all Businesses."
                    android:textColor="#232323"
                    android:textSize="15sp"
                    app:layout_constraintLeft_toLeftOf="@id/gd4"
                    app:layout_constraintRight_toRightOf="@id/gd5"
                    app:layout_constraintTop_toBottomOf="@+id/title_vision" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/mission"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:background="@drawable/curved_corneres_2"
